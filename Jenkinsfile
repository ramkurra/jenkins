pipeline{
    
    agent any

    stages{

        stage('init'){
            steps{
               sh 'echo "in Init" ' 
            }
        }
           stage('build'){
            steps{
               sh 'echo "in Build" ' 
            }
        }

               stage('deploy'){
            steps{
               sh 'echo "in deploy" ' 
            }
        }
       
    }
 
}